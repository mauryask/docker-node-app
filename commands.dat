# Commnd to list all the images 
docker images 

#Command to list all the running containers
docker ps 

#Command to list all the containers running and stopped ones
docker ps -a 

#Command to stop a running container 
docker stop [container_name]

# Command to delete a container 
docker rm [container_name] 

#Command to run a docker image 
docker run --name [container_name] -p [localhost_port]:[container_port] [image_name]
